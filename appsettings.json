{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "ElasticApm": {
    "ServerUrl": "http://localhost:8200",
    "SecretToken": "apm-server-secret-token",
    //"TransactionSampleRate": 1.0,
    "ServiceName": "MinimalAPI",
    "ServiceVersion": "1.0",
    "Environment": "development",
    "CaptureBody": "all",
    "CaptureHeaders": true,
    "CaptureRemoteUser": true

  },
  "AllowedHosts": "*"
}